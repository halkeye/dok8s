{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^namespaces/jenkins/jenkins.yaml$"
      ],
      "matchStrings": [
        "tag: \"(?<currentValue>.*)@(?<currentDigest>.*)\""
      ],
      "depNameTemplate": "halkeye/jenkins",
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    }
  ]
}
