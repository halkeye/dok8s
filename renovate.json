{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^namespaces/jenkins/jenkins.yaml$"
      ],
      "matchStrings": [
        "tag: \"(?<currentValue>[a-z0-9.-]+)@(?<currentDigest>sha256:[a-f0-9]+)\""
      ],
      "depName": "halkeye/jenkins",
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    }
  ]
}
